<?xml version="1.0"?>
<WidgetStore>
    <required>
        <manifest_version>1</manifest_version>				<!-- only use integer-->
        <widget>
            <name>MyWidget</name>
            <version>1.0</version>								<!-- dont use charater only use integer or decimal-->
            <root>./entry.html</root>
        </widget>
        <position kind="git">										<!-- default : "nothing", {"nothing", "git"} -->
			<git>
			    <id>gooddaumi@naver.com</id>
			    <repository>WidgetStore</repository>
			    <tag>1.41</tag>                               <!--default : "" -->
			    <branch>master</branch>                       <!--default : "master"--> <!-- caution : just use one between tag and branch -->
			</git>
		  </position>
     </required>

    <recommanded>
        <support_resolution>yes</support_resolution>      <!--default:"no", {"yes","no"}-->
        <resolution method="ratio"/>                      <!--default:"unsupported" ,{"unsupported","ratio","free","auto"}-->
            <ratio>
                <ratio value="1:1" size="300,300"/>
                <ratio value="3:4" size="300,400"/>
                <ratio value="16:9" size="400,300"/>
            </ratio>
            <free>
                <minWidthSize>300</minWidthSize>
                <minHeightSize>300</minHeightSize>
                <maxWidthSize>900</maxWidthSize>
                <maxHeightSize>900</maxHeightSize>
            </free>           

        <icon-path>./image/icon/title.jpg</icon-path>
    </recommanded>

	    <optional>
    </optional>   
</WidgetStore>

